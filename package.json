{
	"name": "sampleextension",
	"displayName": "sampleExtension",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:sampleextension.helloWorld"
	],
	"main": "./client/out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sampleextension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "helloPoorna",
				"title": "Say hello to poorna"
			},
			{
				"command": "sampleextension.fetch",
				"title": "Fetch definitions"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w"
	},
	"devDependencies": {
		"@types/vscode": "^1.65.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.2"
	},
	"dependencies": {
		"vscode-languageclient": "^7.0.0"
	}
}
