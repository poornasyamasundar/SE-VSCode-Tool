{
	"name": "acspython",
	"displayName": "ACS-python",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:ACS-python-sidebar",
		"onCommand:ACS-python.start",
		"onLanguage:python"
	],
	"main": "./client/out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "ACS-python-sidebar-view",
					"title": "ACS-python",
					"icon": "client/media/logo.svg"
				}
			]
		},
		"views": {
			"ACS-python-sidebar-view": [
				{
					"type": "webview",
					"id": "ACS-python-sidebar",
					"name": "ACS-python",
					"icon": "client/media/logo.svg",
					"contextualTitle": "ACS-python"
				}
			]
		},
		"commands": [
			{
				"command": "ACS-python.start",
				"title": "ACS-python: Start"
			},
			{
				"command": "ACS-python.getSearchResults",
				"title": "z;lfahogs",
				"when": false
			},
			{
				"command": "ACS-python.fetch",
				"title": "ACS-python: Fetch Definitions"
			},
			{
				"command": "ACS-python.fetchSummary",
				"title": "aouhtft98ythyutr8",
				"when": false
			},
			{
				"command": "ACS-python.getSummary",
				"title": "ACS-python: Get Summary"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w"
	},
	"devDependencies": {
		"@types/vscode": "^1.65.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.2"
	},
	"dependencies": {
		"concurrently": "^7.0.0",
		"decoder": "^1.1.6",
		"require": "^2.4.20",
		"rollup": "^2.70.1",
		"string": "^3.3.3",
		"string_decoder": "^1.3.0",
		"svelte": "^3.46.4",
		"vscode-languageclient": "^7.0.0"
	}
}
