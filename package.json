{
	"name": "sampleextension",
	"displayName": "sampleExtension",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:sampleextension.helloWorld",
		"onCommand:sampleextension.searchWorld"
	],
	"main": "./client/out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sampleextension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "helloPoorna",
				"title": "Say hello to poorna"
			},
			{
				"command": "sampleextension.fetch",
				"title": "Fetch definitions"
			},
			{
				"command": "sampleextension.searchWorld",
				"title": "Webview"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\""
	},
	"devDependencies": {
		"@types/vscode": "^1.65.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.2"
	},
	"dependencies": {
		"@rollup/plugin-commonjs": "^21.0.3",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@rollup/plugin-typescript": "^8.3.1",
		"concurrently": "^7.0.0",
		"rollup": "^2.70.1",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.46.4",
		"svelte-preprocess": "^4.10.4",
		"vscode-languageclient": "^7.0.0"
	}
}
